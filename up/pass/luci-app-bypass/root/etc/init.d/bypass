		ln_bin $(first_type redsocks2) redsocks2 -c $VAR/redsocks-by-nf.json >$log_file 2>&1 &
		log "NF  Shunt : Network Tunnel REDIRECT $ntype Started!"
		;;
	esac
	log "NF  Shunt : NF Server $(uci_get_by_name $NF_SERVER alias) $ntype Started!"
}

start_local(){
	rtype=socks
	[ $SO_SERVER = 0 -o "$socks5_start" = 1 ] && return
	local type=$(uci_get_by_name $SO_SERVER type)

	socks5_port=$(uci_get_by_type socks5_proxy local_port 1080)
	local_enable=1
	[ ! -d $VAR ] && mkdir -p $VAR
	case $type in
	ss|ssr)
		gen_config_file $SO_SERVER $rtype
		ss_program="$(first_type ${type}local ${type}-redir)"
		for i in $(seq 1 $threads);do
			ln_bin "$ss_program" ${type}-redir -c  $config_file >$log_file 2>&1 &
		done
		[ $type = ss ] && name=Shadowsocks || name=ShadowsocksR
		log "Socks5 Node:  $name $type Started!"
		;;
	v2ray)
		gen_config_file $SO_SERVER $rtype $socks5_port $socks5_ip
		ln_bin $(first_type xray v2ray) v2ray run -c  $config_file >$log_file 2>&1 &
		log "Socks5 Node:  $($(first_type xray v2ray) version | head -1)  $type Started!"
		;;

	trojan)
		gen_config_file $SO_SERVER $rtype "" $socks5_ip
		ln_bin $(first_type $type) $type --config  $config_file >$log_file 2>&1 &

		log "Socks5 Node:  $($(first_type $type) --version 2>&1 | head -1) , $threads Threads Started!"
		;;
	naiveproxy)
		gen_config_file $SO_SERVER $rtype
		ln_bin $(first_type naive) naive $config_file >$log_file 2>&1 &
		log "Socks5 Node:  $($(first_type naive) --version 2>&1 | head -1) , Threads Started!"
		;;
	hysteria)
		gen_config_file $SO_SERVER $rtype $socks5_port $socks5_ip
		ln_bin $(first_type hysteria) hysteria client --config $config_file >$log_file 2>&1 &
		log "Socks5 Node:  $($(first_type hysteria) version | awk '{print $1,$3}')   $type Started!"
		;;
	tuic)
		gen_config_file $SO_SERVER $rtype $socks5_port $socks5_ip
		ln_bin $(first_type tuic-client) tuic-client --config $config_file >$log_file 2>&1 &
		log "Socks5 Node:  tuic-client $($(first_type tuic-client) --version) Started!"
		;;
	socks5)
		redir_tcp=$threads
		if [ "$(uci_get_by_name $GLOBAL_SERVER auth_enable 0)" == "1" ]; then
			local auth="-a $(uci_get_by_name $GLOBAL_SERVER username) -k $(uci_get_by_name $GLOBAL_SERVER password)"
		fi
		ln_bin $(first_type ipt2socks) ipt2socks $tcp_config_file -R -4 -j $threads -s $(uci_get_by_name $GLOBAL_SERVER server) -p $(uci_get_by_name $GLOBAL_SERVER server_port) -l $tcp_port $auth  >$log_file 2>&1 &
		log "Socks5 Node:  Socks5 $threads Threads Started!"
		;;
	tun)
		redir_tcp=$threads
		$BIN_DIR/genred2config $VAR/redsocks-by-retcp.json vpn $(uci_get_by_name $GLOBAL_SERVER iface br-lan) $local_port
		for i in $(seq 1 $threads);do
			ln_bin $(first_type redsocks2) redsocks2 -c $VAR/redsocks-by-retcp.json >$log_file 2>&1 &
		done
		log "Socks5 Node:  Network Tunnel $threads Threads Started!"
		;;
	esac
	ipset add bypass_wan_ac $socks5_ip >/dev/null 2>&1 &
	log "Socks5 Node: Socks5 Server $(uci_get_by_name $SO_SERVER alias) Started!"
}

gen_dns(){
